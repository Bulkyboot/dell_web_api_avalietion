E,IAAA,CAAA,MAaP;AAbgB,IAAA,UAAU,EAAE,IAa5B;AAbkC,IAAA,YAAY,EAAC,OAa/C;AAbuD,IAAA,gBAAgB,EAAC;AAaxE,GAbZ,E;AAUa,IAAA,KAAK,EAAA,QAAA,CAAC;AAAA,aAEjB,C,WAAA,CAFiB;AAAA,KAAD,C;sBAThB;AAAA,aAAwC,CAAxC,YAAA,CAAwC,iBAAxC,EAAwC;AAAhC,QAAA,KAAK,EAAC,IAA0B;AAArB,QAAA,MAAM,EAAC;AAAc,OAAxC,CAAwC,EACxC,YAAA,CAA4C,iBAA5C,EAA4C;AAApC,QAAA,KAAK,EAAC,MAA8B;AAAvB,QAAA,MAAM,EAAC;AAAgB,OAA5C,CADwC,EAExC,YAAA,CAA0C,iBAA1C,EAA0C;AAAlC,QAAA,KAAK,EAAC,KAA4B;AAAtB,QAAA,MAAM,EAAC;AAAe,OAA1C,CAFwC,EAGxC,YAAA,CAKS,iBALT,EAKS;AALA,QAAA,UAAU,EAAE;AAKZ,OALT,EAA0B;AACb,QAAA,IAAI,EAAA,QAAA,CACb,UADwB,SACxB;AAAA,iBADwB,CACxB,YAAA,CAAmH,iBAAnH,EAAmH;AAA3G,YAAA,IAAI,EAAC,cAAsG;AAAvF,YAAA,KAAK,EAAC,0CAAiF;AAArC,YAAA,OAAK,EAAA,iBAAA,MAAA;AAAA,qBAAE,QAAA,CAAA,UAAA,CAAW,SAAS,CAAC,IAArB,CAAF;AAAA;AAAgC,WAAnH,E,IAAA,E;;AAAA,Y,WAAA,CADwB,EAExB,YAAA,CAAuH,iBAAvH,EAAuH;AAA/G,YAAA,IAAI,EAAC,aAA0G;AAA5F,YAAA,KAAK,EAAC,mCAAsF;AAAjD,YAAA,OAAK,EAAA,iBAAA,MAAA;AAAA,qBAAE,QAAA,CAAA,mBAAA,CAAoB,SAAS,CAAC,IAAV,CAAe,EAAnC,CAAF;AAAA;AAA4C,WAAvH,E,IAAA,E;;AAAA,Y,WAAA,CAFwB,CACxB;AAAA,SADa,CADS;;;;AAAA,OAA1B,CAHwC,CAAxC;AAAA,K;;;;GADF,E;;AAAA,I,SAAA,CAcI,CAfN,CAgBI,CA5EN,C","sourcesContent":["<template>\n\t<Toast />\n\t<Dialog header=\"Confirmation\" v-model:visible=\"displayConfirmation\" :style=\"{width: '350px'}\" :modal=\"true\">\n\t\t<div class=\"confirmation-content\">\n\t\t\t<i class=\"pi pi-exclamation-triangle p-mr-3\" style=\"font-size: 2rem\" />\n\t\t\t<span>Are you sure you want to proceed?</span>\n\t\t</div>\n\t\t<template #footer>\n\t\t\t<Button label=\"No\" icon=\"pi pi-times\" @click=\"cancelAuthor\" class=\"p-button-text\"/>\n\t\t\t<Button label=\"Yes\" icon=\"pi pi-check\" @click=\"deleteAuthor\" class=\"p-button-text\" autofocus />\n\t\t</template>\n\t</Dialog>\n  <div class=\"content\">\n    <div class=\"card-content\">\n      <Card>\n        <template #title>\n          <h1>Author</h1>\n        </template>\n        <template #content> \n          <div class=\"content-filter\">\n            <span class=\"p-field\">\n              <h5>Id</h5>\n              <InputText id=\"id\" type=\"text\" v-model=\"id\" disabled />\n            </span>\n            <span class=\"p-field\">\n              <h5>name</h5>\n              <InputText id=\"title\" type=\"text\" v-model=\"name\" />\n            </span>\n            <span class=\"p-field\">\n              <h5>cpf</h5>\n              <InputNumber id=\"price\" v-model=\"cpf\" />\n            </span>\n            <span class=\"p-field\">\n              <h5>Authors</h5>\n              <!--<Dropdown \n                v-model=\"selectedAuthor\" \n                :options=\"optionsAuthors\" \n                optionLabel=\"name\"  \n                placeholder=\"Select a Author\" \n              />-->\n              <Dropdown \n\t\t\t\t\t\t\t\tv-model=\"selectedAuthor\" \n\t\t\t\t\t\t\t\t:options=\"optionsAuthors\" \n\t\t\t\t\t\t\t\toptionLabel=\"name\" \n\t\t\t\t\t\t\t\t:filter=\"true\" \n\t\t\t\t\t\t\t\tplaceholder=\"Select a Author\" \n\t\t\t\t\t\t\t\t:showClear=\"true\"\n\t\t\t\t\t\t\t>\n\t\t\t\t\t\t\t\t<template #value=\"slotProps\">\n\t\t\t\t\t\t\t\t\t<div class=\"country-item country-item-value\" v-if=\"slotProps.value\">\n\t\t\t\t\t\t\t\t\t\t<div>{{slotProps.value.name}}</div>\n\t\t\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t\t\t\t<span v-else>\n\t\t\t\t\t\t\t\t\t\t{{slotProps.placeholder}}\n\t\t\t\t\t\t\t\t\t</span>\n\t\t\t\t\t\t\t\t</template>\n\t\t\t\t\t\t\t\t<template #option=\"slotProps\">\n\t\t\t\t\t\t\t\t\t<div class=\"country-item\">\n\t\t\t\t\t\t\t\t\t\t<div>{{slotProps.option.name}}</div>\n\t\t\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t\t\t</template>\n            </Dropdown>\n            </span>\n          </div> \n          <div class=\"d-button\">\n            <Button label=\"Save\" @click=\"save\" v-if=\"isSave\"/>\n            <Button label=\"Update\" @click=\"update\" v-else/>\n          </div>    \n        </template>\n      </Card>\n    </div>\n    \n    <div class=\"d-datatable\">\n      <DataTable :value=\"author\" :scrollable=\"true\" scrollHeight=\"400px\" responsiveLayout=\"scroll\">\n        <Column field=\"id\" header=\"ID\"></Column>\n        <Column field=\"name\" header=\"Name\"></Column>\n        <Column field=\"cpf\" header=\"Cpf\"></Column>\n        <Column :exportable=\"false\">\n          <template #body=\"slotProps\">\n            <Button icon=\"pi pi-pencil\" class=\"p-button-rounded p-button-success p-mr-2\" @click=\"editAuthor(slotProps.data)\" />\n            <Button icon=\"pi pi-trash\" class=\"p-button-rounded p-button-warning\" @click=\"confirmDeleteAuthor(slotProps.data.id)\" />\n          </template>\n        </Column>\n        <template #empty>\n            No records found.\n        </template>\n      </DataTable>\n    </div>\n  </div>\n</template>\n\n<script>\nimport BookService from '../services/BookService';\nimport AuthorService from '../services/AuthorService';\nexport default {\n  name: 'Author',\n  data() {\n    return {\n      id: null,\n      title: '',\n      price: null,\n      books: [],\n      optionsAuthors: [],\n      selectedAuthor:null,\n      isSave: true,\n      displayConfirmation: false\n    }\n  },\n  bookService: null,\n  authorService: null,\n  created() {\n    this.authorService = new AuthorService();\n  },\n  async mounted() {\n    await this.requestGetAuthors();\n  },\n  methods: {\n    showSuccess() {\n        this.$toast.add({severity:'success', summary: 'Success Message', detail:'Message Content', life: 3000});\n    },\n    showError() {\n        this.$toast.add({severity:'error', summary: 'Error Message', detail:'Message Content', life: 3000});\n    },\n    async save() {\n      let name = {\n        'name': this.name,\n        'cpf': this.cpf\n      };\n      if(this.selectedAuthor) {\n        book.author = {\n          'id': this.selectedAuthor.id\n        };\n      }\n      await this.requestPostAuthor(author);\n      this.clearField();\n    },\n    async update() {\n      let author = {\n        'name': this.name,\n        'cpf': this.cpf\n      };\n      if(this.selectedAuthor) {\n        book.author = {\n          'id': this.selectedAuthor.id\n        };\n      }\n      await this.requestPutAuthor(this.id, author);\n      this.clearField();\n      this.isSave = true;\n    },\n    async deleteAuthor(){\n      await this.requestDeleteAuthor(this.id);\n      this.displayConfirmation = false;\n      this.clearField();\n      this.isSave = true;\n    },\n    cancelAuthor() {\n      this.id= null;\n      this.displayConfirmation = false;\n      this.clearField();\n      this.isSave = true;\n    },\n    clearField() {\n      this.id = null,\n      this.title = '',\n      this.price = null,\n      this.selectedAuthor = null\n    },\n    editAuthor(data) {\n      this.name = data.name;\n      this.cpf = data.cpf;\n      this.id = data.id;\n      let author = {...data.author};\n      if(author.name !== '-') {\n        this.selectedAuthor = {\n            id: author.id,\n            name: author.name    \n        };\n      } else {\n          this.selectedAuthor = null;\n      }\n      this.isSave = false;\n    },\n    confirmDeleteAuthor(value) {\n      this.id = value;\n      this.displayConfirmation = true;\n    },\n    async requestGetAuthor() {\n      await this.bookService.getAuthor()\n        .then(response => {\n          let data = response.data;\n          this.author = [];\n          data.forEach(book => {\n            let bookLocal = {...book};\n            bookLocal.author =  bookLocal.author ? bookLocal.author : { name: '-'};\n            this.books.push({...bookLocal});\n          });\n        })\n        .catch(() => {\n          console.log('Ocorreu um erro!');\n        });\n    },\n    async requestGetAuthors() {\n      await this.authorService.getAuthors()\n        .then(response => {\n          let data = response.data;\n          data.forEach(author => {\n            this.optionsAuthors = [];\n            this.optionsAuthors.push({\n              id: author.id,\n              name: author.name\n            })\n          });\n        })\n        .catch(() => {\n          this.optionsAuthors=[];\n          this.showError();\n        })\n    },\n    async requestPostAuthor(author = {}) {\n      await this.authorService.postBooks(author)\n        .then(() => {\n          this.requestGetAuthor();\n          this.showSuccess();\n        })\n        .catch(error => {\n          this.$toast.add({severity:'error', summary: 'Error Register', detail:`${error.response.data.title}`, life: 3000});\n        })\n    },\n    async requestPutAuthor(authorId, author = {}) {\n      await this.authorService.putAuthor(authorId, author)\n        .then(() => {\n          this.requestGetBooks();\n          this.showSuccess();\n        })\n        .catch(() => {\n          this.showError();\n        })\n    },\n    async requestDeleteAuthor(authorName) {\n      await this.authorService.deleteAuthor(authorNamen)\n        .then(() => {\n          this.requestGetBooks();\n          this.showSuccess();\n        })\n        .catch(() => {\n          this.showError();\n        })\n    }\n  }\n}\n</script>"],"sourceRoot":""}]}